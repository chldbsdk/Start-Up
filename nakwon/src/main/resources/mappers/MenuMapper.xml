<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nakwon.mapper.MenuMapper">
	<insert id="insert">
		insert into Menu (MenuTitle, MenuContent,MenuImg,MenuPrice,MenuIngredients,MenuAllergy)
		values(#{MenuTitle},#{MenuContent},#{MenuImg},#{MenuPrice},#{MenuIngredients},#{MenuAllergy})
	</insert>
</mapper>
